		flex-shrink: 0;
		position: relative;
		width: 30rpx;
		height: 30rpx;
		top: 15rpx;
		border-radius: 50%;
		background-color: #bbb;
}
.uni-timeline-item-divider::before,
	.uni-timeline-item-divider::after {
		position: absolute;
		left: 15rpx;
		width: 1rpx;
		height: 100vh;
		content: '';
		background: inherit;
}
.uni-timeline-item-divider::before {
		bottom: 100%;
}
.uni-timeline-item-divider::after {
		top: 100%;
}
.uni-timeline-last-item .uni-timeline-item-divider:after {
		display: none;
}
.uni-timeline-first-item .uni-timeline-item-divider:before {
		display: none;
}
.uni-timeline-item .uni-timeline-item-content {
		padding-left: 20rpx;
}
.uni-timeline-last-item .bottom-border::after{
		display: none;
}
